            <F N="Widgets.h"/>
        </Folder>
        <Folder
            Name="Tools"
            Filters="*.py"
            GUID="{3935C99B-B3E5-41A0-9D1B-CAEB26422F2D}">
            <F N="../.gitignore"/>
            <F N="adr.bat"/>
            <F
                N="Makefile"
                Type="Makefile"/>
            <F N="../README.md"/>
        </Folder>
    </Files>
